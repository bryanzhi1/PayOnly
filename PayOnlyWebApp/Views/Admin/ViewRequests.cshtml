@model IEnumerable<PayOnlyWebApp.Models.CashOutRequest>

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>View Cash Out Requests</title>
</head>
<body>
    <table class="table table-striped table-bordered">
        
            <thead>Cash Out ID</thead>
            <thead>Merchant ID</thead>
            <thead>Merchant Name</thead>
            <thead>Bank Name</thead>
            <thead>Account Name</thead>
            <thead>Amount</thead>
            <thead>Status</thead>
            <tbody class="thead-dark">
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@item.CashOutID.ToString()</td>
                        <td>@item.MerchantID.ToString()</td>
                        <td>@item.BankName</td>
                        <td>@item.AccountName</td>
                        <td>@item.AccountNumber</td>
                        <td>@item.Amount.ToString()</td>
                        <td>@item.Status</td>
                    </tr>
                }
            </tbody>
    </table>
   </body>
</html>
